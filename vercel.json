{
    "version" : 2,
    "builds" : [
        {
            "src" : "poll/wsgi.py",
            "use" : "@vercel/python",
            "config" : { "maxLambdasize":"15mb","runtime":"python3.10.0"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config" :{
                "distDir" : "build_files"
            }

        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"poll/wsgi.py"
        }
    ]
}